{
  "name": "jiji",
  "version": "0.0.0",
  "author": "Emmanuelle Fernandes",
  "license": "MIT",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "targets": {
    "main": {
      "source": "src/index.ts",
      "context": "node",
      "optimize": true
    },
    "module": {
      "source": "src/index.ts",
      "context": "node",
      "optimize": true
    },
    "cli": {
      "source": "src/cli.ts",
      "context": "node",
      "optimize": true,
      "isLibrary": false,
      "includeNodeModules": [
        "sade",
        "mri"
      ]
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "uvu -r ts-node/register tests -i cases",
    "test:unit": "uvu -r ts-node/register tests/unit",
    "test:e2e": "uvu -r ts-node/register tests/e2e -i cases",
    "test:cov": "c8 --all --src=src --include=src -r text -r html yarn test",
    "test:single": "ts-node"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "sade": "^1.8.1"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.4.0",
    "@parcel/transformer-typescript-types": "2.4.0",
    "@types/node": "^17.0.23",
    "c8": "^7.11.0",
    "parcel": "^2.4.0",
    "prettier": "^2.6.0",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "uvu": "^0.5.3"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": [
    "src/cli.ts",
    "dist/cli.js"
  ]
}
